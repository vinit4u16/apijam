<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="httpbin">
    <Description/>
    <FaultRules/>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>SpikeArrest</Name>
            </Step>
            <Step>
                <Name>DisableTargetCopyPathSuffix</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <Flows/>
    <HTTPTargetConnection>
        <Properties/>
        <URL>http://httpbin.org/get</URL>
    </HTTPTargetConnection>
</TargetEndpoint>